{
  "name": "toastinette.js",
  "license": "MIT",
  "private": false,
  "type": "module",
  "version": "0.0.8",
  "unpkg": "dist/toastinette.umd.cjs",
  "main": "./dist/toastinette.umd.cjs",
  "module": "./dist/toastinette.js",
  "exports": {
    ".": {
      "import": "./dist/toastinette.js",
      "require": "./dist/toastinette.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "description": "Vanilla JS notifications with SSR FlashBag support via DOM",
  "author": "Nicolas Lopes",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolaslps/toastinette.js"
  },
  "keywords": [
    "notification",
    "toast",
    "alert",
    "toastinette"
  ],
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite",
    "prettier": "prettier --check .",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.3.1",
    "prettier": "2.8.0",
    "sass": "^1.56.1",
    "typescript": "*",
    "vite": "^3.2.3"
  },
  "dependencies": {}
}
